<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_marginTop="40dp"
    tools:context=".TenantProfileFragment">


    <ScrollView
        android:id="@+id/search_scrollview"
        android:layout_width="match_parent"
        android:layout_height="match_parent">


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginLeft="8dp"
            android:layout_marginRight="10dp"
            android:orientation="vertical">


            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:orientation="horizontal">

                    <TextView
                        android:id="@+id/profile_tenant_email_address_textview"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="1.1"
                        android:text="Email Address"
                        android:textColor="#000000"
                        android:textColorHint="#000000"
                        android:textStyle="bold" />

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="1.3"
                        android:orientation="vertical">

                        <Button
                            android:id="@+id/profile_tenant_email_address_button"
                            style="@style/Widget.MaterialComponents.Button.TextButton"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:enabled="false" />

                        <Button
                            android:id="@+id/profile_tenant_email_address_cancel_button"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="undo"
                            android:visibility="gone" />
                    </LinearLayout>

                </LinearLayout>

                <EditText
                    android:id="@+id/profile_tenant_email_address_editText"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1.1"
                    android:background="#FFFFFF"
                    android:ems="10"
                    android:enabled="false"
                    android:inputType="textPersonName"
                    android:padding="5dp"
                    android:textColor="#000000" />

            </LinearLayout>


            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:orientation="horizontal">

                    <TextView
                        android:id="@+id/profile_tenant_first_name_textView"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="1.1"
                        android:text="First Name"
                        android:textColor="#000000"
                        android:textColorHint="#000000"
                        android:textStyle="bold" />


                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="1.3"
                        android:orientation="vertical">

                        <Button
                            android:id="@+id/profile_tenant_first_name_button"
                            style="@style/Widget.MaterialComponents.Button.TextButton"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="Edit" />

                        <Button
                            android:id="@+id/profile_tenant_first_name_cancel_button"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="undo"
                            android:visibility="gone" />

                    </LinearLayout>

                </LinearLayout>

                <EditText
                    android:id="@+id/profile_tenant_first_name_editText"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1.1"
                    android:background="#FFFFFF"
                    android:ems="10"
                    android:enabled="false"
                    android:inputType="textPersonName"
                    android:padding="5dp"
                    android:textColor="#000000" />

            </LinearLayout>


            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:orientation="horizontal">

                    <TextView
                        android:id="@+id/profile_tenant_last_name_textView"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="1.1"
                        android:text="Last Name"
                        android:textColor="#000000"
                        android:textColorHint="#000000"
                        android:textStyle="bold" />


                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="1.3"
                        android:orientation="vertical">

                        <Button
                            android:id="@+id/profile_tenant_last_name_button"
                            style="@style/Widget.MaterialComponents.Button.TextButton"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="Edit" />

                        <Button
                            android:id="@+id/profile_tenant_last_name_cancel_button"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="undo"
                            android:visibility="gone" />

                    </LinearLayout>

                </LinearLayout>

                <EditText
                    android:id="@+id/profile_tenant_last_name_editText"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1.1"
                    android:background="#FFFFFF"
                    android:ems="10"
                    android:enabled="false"
                    android:inputType="textPersonName"
                    android:padding="5dp"
                    android:textColor="#000000" />

            </LinearLayout>


            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:orientation="horizontal">

                    <TextView
                        android:id="@+id/profile_tenant_gender_textView"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="1.1"
                        android:text="Gender"
                        android:textColor="#000000"
                        android:textColorHint="#000000"
                        android:textStyle="bold" />


                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="1.3"
                        android:orientation="vertical">

                        <Button
                            android:id="@+id/profile_tenant_gender_button"
                            style="@style/Widget.MaterialComponents.Button.TextButton"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="Edit" />

                        <Button
                            android:id="@+id/profile_tenant_gender_cancel_button"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="undo"
                            android:visibility="gone" />

                    </LinearLayout>

                </LinearLayout>

                <Spinner
                    android:id="@+id/profile_tenant_gender_spinner"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1.1"
                    android:padding="5dp" />

            </LinearLayout>


            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:orientation="horizontal">

                    <TextView
                        android:id="@+id/profile_tenant_nationality_textView"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="1.1"
                        android:text="Nationality"
                        android:textColor="#000000"
                        android:textColorHint="#000000"
                        android:textStyle="bold" />


                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="1.3"
                        android:orientation="vertical">

                        <Button
                            android:id="@+id/profile_tenant_nationality_button"
                            style="@style/Widget.MaterialComponents.Button.TextButton"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="Edit" />

                        <Button
                            android:id="@+id/profile_tenant_nationality_cancel_button"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="undo"
                            android:visibility="gone" />

                    </LinearLayout>

                </LinearLayout>

                <Spinner
                    android:id="@+id/profile_tenant_nationality_spinner"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1.1"
                    android:padding="5dp" />

            </LinearLayout>


            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:orientation="horizontal">

                    <TextView
                        android:id="@+id/profile_tenant_gross_monthly_salary_textView"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="1.1"
                        android:text="Monthly Salary"
                        android:textColor="#000000"
                        android:textColorHint="#000000"
                        android:textStyle="bold" />


                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="1.3"
                        android:orientation="vertical">

                        <Button
                            android:id="@+id/profile_tenant_gross_monthly_salary_button"
                            style="@style/Widget.MaterialComponents.Button.TextButton"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="Edit" />

                        <Button
                            android:id="@+id/profile_tenant_gross_monthly_salary_cancel_button"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="undo"
                            android:visibility="gone" />

                    </LinearLayout>

                </LinearLayout>

                <EditText
                    android:id="@+id/profile_tenant_gross_monthly_salary_editText"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1.1"
                    android:background="#FFFFFF"
                    android:ems="10"
                    android:enabled="false"
                    android:inputType="numberDecimal"
                    android:padding="5dp"
                    android:textColor="#000000" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:orientation="horizontal">

                    <TextView
                        android:id="@+id/profile_tenant_occupation_textView"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="1.1"
                        android:text="Occupation"
                        android:textColor="#000000"
                        android:textColorHint="#000000"
                        android:textStyle="bold" />

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="1.3"
                        android:orientation="vertical">

                        <Button
                            android:id="@+id/profile_tenant_occupation_button"
                            style="@style/Widget.MaterialComponents.Button.TextButton"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="Edit" />

                        <Button
                            android:id="@+id/profile_tenant_occupation_cancel_button"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="undo"
                            android:visibility="gone" />

                    </LinearLayout>

                </LinearLayout>

                <EditText
                    android:id="@+id/profile_tenant_occupation_editText"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1.1"
                    android:background="#FFFFFF"
                    android:ems="10"
                    android:enabled="false"
                    android:inputType="textPersonName"
                    android:padding="5dp"
                    android:textColor="#000000" />

            </LinearLayout>


            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:orientation="horizontal">

                    <TextView
                        android:id="@+id/profile_tenant_family_size_textView"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:text="Family Size"
                        android:textColor="#000000"
                        android:textColorHint="#000000"
                        android:textStyle="bold" />

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="1.2"
                        android:orientation="vertical">

                        <Button
                            android:id="@+id/profile_tenant_family_size_button"
                            style="@style/Widget.MaterialComponents.Button.TextButton"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="Edit" />

                        <Button
                            android:id="@+id/profile_tenant_family_size_cancel_button"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="Undo"
                            android:visibility="gone" />

                    </LinearLayout>

                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:orientation="horizontal">

                    <Button
                        android:id="@+id/profile_tenant_family_size_dec_button"
                        style="@style/Widget.MaterialComponents.Button.OutlinedButton"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_margin="0dp"
                        android:layout_weight="1"
                        android:enabled="false"
                        android:padding="5dp"
                        android:text="-" />

                    <EditText
                        android:id="@+id/profile_tenant_family_size_editText"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:background="#FFFFFF"
                        android:enabled="false"
                        android:inputType="number"
                        android:padding="5dp"
                        android:textColor="#000000" />

                    <Button
                        android:id="@+id/profile_tenant_family_size_inc_button"
                        style="@style/Widget.MaterialComponents.Button.OutlinedButton"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_margin="0dp"
                        android:layout_weight="1"
                        android:enabled="false"
                        android:padding="5dp"
                        android:text="+" />
                </LinearLayout>

            </LinearLayout>


            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:orientation="horizontal">

                    <TextView
                        android:id="@+id/profile_tenant_current_residence_country_textView"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:text="Current County"
                        android:textColor="#000000"
                        android:textColorHint="#000000"
                        android:textStyle="bold" />


                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="1.3"
                        android:orientation="vertical">

                        <Button
                            android:id="@+id/profile_tenant_current_residence_country_button"
                            style="@style/Widget.MaterialComponents.Button.TextButton"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="Edit" />

                        <Button
                            android:id="@+id/profile_tenant_current_residence_country_cancel_button"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="Undo"
                            android:visibility="gone" />

                    </LinearLayout>


                </LinearLayout>

                <Spinner
                    android:id="@+id/profile_tenant_current_residence_country_spinner"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:padding="5dp" />

            </LinearLayout>


            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:orientation="horizontal">

                    <TextView
                        android:id="@+id/profile_tenant_city_textView"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:text="City"
                        android:textColor="#000000"
                        android:textColorHint="#000000"
                        android:textStyle="bold" />


                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="1.3"
                        android:orientation="vertical">

                        <Button
                            android:id="@+id/profile_tenant_city_button"
                            style="@style/Widget.MaterialComponents.Button.TextButton"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="Edit" />

                        <Button
                            android:id="@+id/profile_tenant_city_cancel_button"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="Undo"
                            android:visibility="gone" />

                    </LinearLayout>

                </LinearLayout>

                <Spinner
                    android:id="@+id/profile_tenant_city_spinner"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:padding="5dp" />

            </LinearLayout>


            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:orientation="horizontal">

                    <TextView
                        android:id="@+id/profile_tenant_phone_number_textView"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:text="Phone Number"
                        android:textColor="#000000"
                        android:textColorHint="#000000"
                        android:textStyle="bold" />

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="1.3"
                        android:orientation="vertical">

                        <Button
                            android:id="@+id/profile_tenant_phone_number_button"
                            style="@style/Widget.MaterialComponents.Button.TextButton"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="Edit" />

                        <Button
                            android:id="@+id/profile_tenant_phone_number_cancel_button"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="Undo"
                            android:visibility="gone" />

                    </LinearLayout>


                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:orientation="horizontal">

                    <ImageView
                        android:id="@+id/profile_tenant_area_code_imageview"
                        android:layout_width="match_parent"
                        android:layout_height="30dp"
                        android:layout_gravity="center"
                        android:layout_marginTop="5dp"
                        android:layout_weight="1.4"
                        android:padding="5dp"
                        android:src="@drawable/flag1" />

                    <TextView
                        android:id="@+id/profile_tenant_area_code_textview"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="top"
                        android:background="#FFFFFF"
                        android:padding="5dp"
                        android:text="area code"
                        android:textColor="#000000"
                        android:textColorHint="#000000"
                        android:textSize="18sp" />

                    <EditText
                        android:id="@+id/profile_tenant_phone_number_editText"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:background="#FFFFFF"
                        android:ems="10"
                        android:enabled="false"
                        android:inputType="phone"
                        android:padding="5dp"
                        android:textColor="#000000" />

                </LinearLayout>

            </LinearLayout>


            <LinearLayout
                android:id="@+id/profile_tenant_change_password_layout"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical"
                android:visibility="gone">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="7dp"
                    android:orientation="horizontal">

                    <TextView
                        android:id="@+id/profile_tenant_password_textView"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:paddingTop="5dp"
                        android:text="Current Password"
                        android:textColor="#000000"
                        android:textColorHint="#000000"
                        android:textStyle="bold" />

                    <EditText
                        android:id="@+id/profile_tenant_password_editText"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:background="#FFFFFF"
                        android:ems="10"
                        android:hint="Current Password"
                        android:inputType="textPassword"
                        android:padding="5dp"
                        android:textColor="#000000" />

                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="7dp"
                    android:orientation="horizontal">

                    <TextView
                        android:id="@+id/profile_tenant_new_password_textView"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:paddingTop="5dp"
                        android:text="New Password"
                        android:textColor="#000000"
                        android:textColorHint="#000000"
                        android:textStyle="bold" />

                    <EditText
                        android:id="@+id/profile_tenant_new_password_editText"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:background="#FFFFFF"
                        android:ems="10"
                        android:hint="New Password"
                        android:inputType="textPassword"
                        android:padding="5dp"
                        android:textColor="#000000" />

                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="7dp"
                    android:orientation="horizontal">

                    <TextView
                        android:id="@+id/profile_tenant_confirm_new_password_textView"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:hint="Confirm New Password"
                        android:paddingTop="5dp"
                        android:text="Confirm Password"
                        android:textColor="#000000"
                        android:textColorHint="#000000"
                        android:textStyle="bold" />

                    <EditText
                        android:id="@+id/profile_tenant_confirm_new_password_editText"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:background="#FFFFFF"
                        android:ems="10"
                        android:inputType="textPassword"
                        android:padding="5dp"
                        android:textColor="#000000" />

                </LinearLayout>

            </LinearLayout>


            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="right"
                android:layout_marginTop="5dp"
                android:orientation="horizontal">

                <Button
                    android:id="@+id/profile_tenant_change_password_button"
                    style="@style/Widget.MaterialComponents.Button.TextButton"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="right"
                    android:layout_weight="1"
                    android:gravity="center_horizontal|center_vertical"
                    android:text="Change Password" />


                <Button
                    android:id="@+id/profile_tenant_cancel_change_password_button"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="center_horizontal|center_vertical"
                    android:text="Undo"
                    android:visibility="gone" />

            </LinearLayout>


        </LinearLayout>

    </ScrollView>


</FrameLayout>